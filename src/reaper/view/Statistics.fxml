<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER">
         <children>
            <HBox alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <PieChart fx:id="chartOverviewStatus" />
                        <TableView fx:id="statsTypeTable" prefHeight="200.0" prefWidth="200.0">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                           <columns>
                              <TableColumn fx:id="statsTypeTypeColumn" prefWidth="75.0" text="Type" />
                              <TableColumn fx:id="statsTypeCountColumn" prefWidth="75.0" text="Count" />
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <PieChart fx:id="chartCodesStatus" />
                        <TableView fx:id="statsCodeTable" prefHeight="200.0" prefWidth="200.0">
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                           <columns>
                              <TableColumn fx:id="statsCodeCodeColumn" prefWidth="75.0" text="Code" />
                              <TableColumn fx:id="statsCodeCountColumn" prefWidth="75.0" text="Count" />
                           </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
